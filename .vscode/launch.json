{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Your own server",
            "request": "launch",
            "type": "al", //Must be set to ".al". Required by Visual Studio Code.
            "environmentType": "OnPrem", //Possible values are OnPrem, Sandbox, or Production.
            "server": "http://bc17",
            "serverInstance": "BC",
            "authentication": "UserPassword",
            "startupObjectId": 22, // Specifies the ID of the object to open after publishing. Only objects of type Page and Table are currently supported.
            "startupObjectType": "Page",//Specifies whether the object to open after publishing
            "breakOnError": true, //Specifies whether to break on errors when debugging. 
            "launchBrowser": true,
            "enableLongRunningSqlStatements": true,
            "enableSqlInformationDebugger": true,
            "tenant": "default"
        }
    ]
}